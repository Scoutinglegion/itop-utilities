# Mysql credentials and host
# Change this according your mysql server instance
export MYSQL_USER=replace-for-your-user
export MYSQL_PASS=replace-for-your-password
export MYSQL_HOSTNAME=replace-for-your-mysql

# Itop server and ssh user to access it (passwordless ssh access has to be configured)
# Change this according your itop instance
# By default this point the demo site by combodo
export ITOP_SERVER=demo.combodo.com
export INSTALLATION_DIRECTORY=simple

export ITOP_USER=admin
export ITOP_PASS=admin

# http or https? 
HTTPS=Y
